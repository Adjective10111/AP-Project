<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="595.0" minWidth="520.0" style="-fx-background-color: #f2e2a6;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="graphics.Controller">
    <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onMouseClicked="#gotoGameModes" text="Back" AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="14.0">
        <font>
            <Font name="Arial Rounded MT Bold" size="17.0" />
        </font>
    </Button>
    <Label layoutX="16.0" layoutY="328.0" prefHeight="21.0" prefWidth="153.0" text="Enter the names:" textFill="#402e07" AnchorPane.topAnchor="328.0">
        <font>
            <Font name="Bookman Old Style" size="18.0" />
        </font>
    </Label>
    <Button layoutX="387.0" layoutY="548.0" mnemonicParsing="false" onAction="#cup" prefWidth="119.0" text="Finish">
        <font>
            <Font name="Arial Rounded MT Bold" size="17.0" />
        </font>
    </Button>
    <TextField layoutX="14.0" layoutY="349.0" prefHeight="33.0" prefWidth="367.0" promptText="divide names by enter" />
    <Label fx:id="names" alignment="CENTER" layoutX="15.0" layoutY="56.0" prefHeight="271.0" prefWidth="490.0" wrapText="true" AnchorPane.bottomAnchor="268.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="56.0">
        <font>
            <Font name="Arial Rounded MT Bold" size="15.0" />
        </font>
    </Label>
   <TextField fx:id="number_of_bots" layoutX="16.0" layoutY="403.0" prefHeight="33.0" prefWidth="153.0" promptText="Enter the number of bots" />
   <Label layoutX="16.0" layoutY="382.0" prefHeight="21.0" prefWidth="153.0" text="If you want to add bots" textFill="#402e07">
      <font>
         <Font name="Bookman Old Style" size="13.0" />
      </font>
   </Label>
</AnchorPane>
