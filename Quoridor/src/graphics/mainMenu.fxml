<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane minHeight="595.0" minWidth="520.0" style="-fx-background-color: #f2e2a6;" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="graphics.Controller">
   <Label layoutX="62.0" layoutY="58.0" prefHeight="38.0" prefWidth="415.0" text="Welcome to Quoridor" textFill="#402e07" AnchorPane.topAnchor="58.0">
